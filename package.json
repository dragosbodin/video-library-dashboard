{
  "name": "video-library-dashboard",
  "version": "0.1.0",
  "engines": {
    "node": ">=22"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development vite --host",
    "dev:server": "nodemon src/server/index.ts",
    "dev": "cross-env NODE_ENV=development concurrently 'npm:dev:client' 'npm:dev:server' 'npm:db:studio'",
    "build:client": "tsc --project tsconfig.build.json && vite build",
    "build:server": "tsc --project tsconfig.server.build.json && ts-add-js-extension --dir=dist/src/server",
    "build:clean": "rm -rf dist",
    "build": "npm run build:clean && npm run build:client && npm run build:server",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "cross-env BROWSER=none prisma studio",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test vitest"
  },
  "dependencies": {
    "@ant-design/icons": "^ 5.6.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@prisma/client": "^6.13.0",
    "@tanstack/react-query": "^5.84.1",
    "@trpc/client": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@trpc/tanstack-react-query": "^11.4.3",
    "antd": "^5.26.7",
    "classnames": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "qs": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.7.1",
    "superjson": "^2.2.2",
    "winston": "^3.17.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.84.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "extensionless": "^1.9.9",
    "msw": "^2.10.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "sass": "^1.89.2",
    "stylelint": "^16.23.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-prettier": "^5.0.3",
    "ts-add-js-extension": "^1.6.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
